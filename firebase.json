{
  "hosting": [
    {
      "target": "landing-page",
      "public": "firebase/hosting/landing-page",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "firestore": {
    "rules": "firebase/firestore.rules",
    "indexes": "firebase/firestore.indexes.json"
  },
  "emulators": {
    "firestore": {
      "port": 11000
    },
    "hosting": {
      "port": 12000
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
